{
    "name": "portal-aws",
    "version": "0.0.0",
    "description": "",
    "main": "index.js",
    "author": "author",
    "license": "ISC",
    "scripts": {
        "dev": "nodemon src/server.js",
        "start": "npm install && npx sequelize-cli db:migrate && npm run deploy && nodemon deploy/server.js",
        "deploy": "sucrase ./src -d ./deploy --transforms imports",
        "migration": "yarn sequelize migration:create",
        "migration:undo": "yarn sequelize db:migrate:undo",
        "migrate": "yarn sequelize db:migrate",
        "test:cov": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandles -- --coverage -c jest-unit.config.js",
        "test:unit": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandles -- --watchAll -c jest-unit.config.js",
        "test:staged": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandle -- --findRelatedTests --silent -c jest-unit.config.js",
        "pretest:integration": "cross-env NODE_ENV=teste yarn migrate",
        "test:integration": "cross-env NODE_ENV=teste yarn jest -- --watch -c jest-integration-config.js --detectOpenHandles",
        "posttest:integration": "cross-env NODE_ENV=teste yarn migration:undo"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@sentry/node": "^6.2.1",
        "@types/express": "^4.17.11",
        "@types/swagger-ui-express": "^4.1.2",
        "aws-sdk": "^2.841.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.3",
        "helmet": "^4.4.1",
        "husky": "^4.3.8",
        "is-port-reachable": "^3.0.0",
        "jsonwebtoken": "^8.5.1",
        "nodemon": "^2.0.7",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.4.0",
        "sequelize-cli": "^6.2.0",
        "sqlite": "^4.0.19",
        "sucrase": "^3.17.0",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.1.6",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@jest-mock/express": "^1.3.0",
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/jest": "^26.0.20",
        "babel-jest": "^26.6.3",
        "cross-env": "^7.0.3",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-prettier-config": "^1.0.1",
        "git-commit-msg-linter": "^3.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "mock-req-res": "^1.2.0",
        "prettier": "^2.2.1",
        "sinon": "^9.2.4",
        "ts-jest": "^26.4.4"
    }
}
